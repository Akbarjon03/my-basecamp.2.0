<br/>
<div class="card">
  <div class="card-header">
    <h1>Message</h1>
  </div>

  <div class="card-body">
    <div id="msgs">
      <% @msgs.each do |msg| %>
        <%= render msg %>
        <p>
          <%= link_to "Show this Message", msg, class:"editslinks" %>
        </p>
      <% end %>
    </div>
    <%= link_to "New Message", new_project_msg_path(@project) , class:"editslinks" %> <br/>
    <%= link_to "Back", project_path(@project) , class:"editslinks" %>
  </div>
</div>